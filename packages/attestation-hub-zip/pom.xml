            <version>${project.version}</version>
            <classifier>feature</classifier>
            <type>zip</type>
        </dependency>
        <dependency>
            <groupId>com.intel.mtwilson.core</groupId>
            <artifactId>mtwilson-core-html5-login-token</artifactId>
            <version>${project.version}</version>
